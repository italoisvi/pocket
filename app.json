{
  "expo": {
    "name": "Pocket",
    "slug": "pocket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "pocket",
    "extra": {
      "eas": {
        "projectId": "3f8676c7-50be-4b5e-a3a0-c49dde31197d"
      }
    },
    "splash": {
      "image": "./assets/images/Pocket.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gladius.pocket",
      "scheme": "pocket",
      "buildNumber": "20",
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa acessar sua c√¢mera para escanear comprovantes de gastos.",
        "NSPhotoLibraryUsageDescription": "Este app precisa acessar sua galeria de fotos para selecionar comprovantes de gastos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gladius.pocket",
      "scheme": "pocket",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "pocket"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "pocket://"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "gladius-gs"
        }
      ]
    ]
  }
}
